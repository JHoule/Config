[alias]
	ci = commit -a
	co = checkout
	st = status
	br = branch
	mg = merge --no-commit
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>

[color]
	ui = auto

[core]
	autocrlf = false
	fileMode = false
